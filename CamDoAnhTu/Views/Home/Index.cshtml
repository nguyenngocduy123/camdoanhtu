@using CamDoAnhTu.Models
@model Customer
@{
    List<Loan> lst = (List<Loan>)ViewData["Loan"];
    User us = CamDoAnhTu.Helper.Helper.GetUserInfo();
}

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Cầm đồ Anh Tú</h2>

<div id="plan-info">

    @if (us.id_cuahang == -1)
    {
        @Html.Action("Management", "Home", new { type = -1 })
        @Html.Action("Management", "Home", new { type = -2 })
        @Html.Action("Management", "Home", new { type = 1 })
        @Html.Action("Management", "Home", new { type = 2 })
        @Html.Action("Management", "Home", new { type = 3 })
        @Html.Action("Management", "Home", new { type = 4 })
        @Html.Action("Management", "Home", new { type = 5 })
        @Html.Action("Management", "Home", new { type = 6 })
        @Html.Action("XE1", "Home")
    }
    else if (us.id_cuahang == 1)
    {
        @Html.Action("Management", "Home", new { type = 1 })
    }
    else if (us.id_cuahang == 2)
    {
        @Html.Action("Management", "Home", new { type = 2 })
    }
    else if (us.id_cuahang == 3)
    {
        @Html.Action("Management", "Home", new { type = 3 })
    }
    else if (us.id_cuahang == 4)
    {
        @Html.Action("Management", "Home", new { type = 4 })
    }
    else if (us.id_cuahang == 5)
    {
        @Html.Action("Management", "Home", new { type = 5 })
    }
    else if (us.id_cuahang == 6)
    {
        @Html.Action("Management", "Home", new { type = 6 })
    }


</div>